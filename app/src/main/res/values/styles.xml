<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="mart_estate">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">32dp</item>
        <item name="android:fontFamily">@font/nunito_extrabold</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">24sp</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.CornerSize8Percent" parent="">
        <item name="cornerSize">8%</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.CornerSize8Percent_detail" parent="">
        <item name="cornerSizeBottomLeft">50dp</item>
        <item name="cornerSizeBottomRight">50dp</item>
    </style>

    <style name="icon_button_bottom" parent="Widget.Material3.Button.ElevatedButton">
        <item name="android:layout_width">257dp</item>
        <item name="android:layout_height">55dp</item>
        <item name="android:backgroundTint">@color/get_started_button</item>
        <item name="android:fontFamily">@font/roboto_medium</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">22sp</item>
        <item name="android:visibility">visible</item>
        <item name="icon">@drawable/get_started_button_icon</item>
        <item name="iconGravity">textEnd</item>
        <item name="iconTint">@null</item>
    </style>
</resources>